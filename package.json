{
  "name": "cooking-blog",
  "private": true,
  "description": "A cooking blog",
  "version": "0.1.0",
  "author": "Aaron McTavish",
  "dependencies": {
    "gatsby": "^2.23.0",
    "gatsby-image": "^2.4.6",
    "gatsby-plugin-codegen": "1.1.0",
    "gatsby-plugin-manifest": "^2.4.10",
    "gatsby-plugin-react-helmet": "^3.3.3",
    "gatsby-plugin-sass": "2.3.3",
    "gatsby-plugin-sharp": "^2.6.10",
    "gatsby-remark-images": "3.3.9",
    "gatsby-source-filesystem": "^2.3.10",
    "gatsby-transformer-remark": "2.8.14",
    "gatsby-transformer-sharp": "^2.5.4",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-helmet": "^6.0.0",
    "sass": "1.26.8"
  },
  "devDependencies": {
    "@types/react": "16.9.35",
    "@types/react-helmet": "6.0.0",
    "@typescript-eslint/eslint-plugin": "3.1.0",
    "@typescript-eslint/parser": "3.1.0",
    "eslint": "7.1.0",
    "eslint-config-prettier": "6.11.0",
    "eslint-plugin-jsdoc": "27.0.4",
    "eslint-plugin-jsx-a11y": "6.2.3",
    "eslint-plugin-prettier": "3.1.3",
    "eslint-plugin-react": "7.20.0",
    "gh-pages": "3.0.0",
    "markdown-spellcheck": "1.3.1",
    "prettier": "2.0.5",
    "typescript": "3.9.3"
  },
  "license": "MIT",
  "scripts": {
    "build": "gatsby build",
    "deploy": "gatsby build --prefix-paths && gh-pages -d public",
    "develop": "gatsby develop -H $(hostname) -p 8000",
    "format": "npm run format:eslint && npm run format:prettier && npm run format:spelling",
    "format:eslint": "eslint --fix \"**/*.{js,jsx,ts,tsx,json}\"",
    "format:prettier": "prettier --write \"**/*.{js,jsx,ts,tsx,json,md,scss}\"",
    "format:spelling": "mdspell './blog/**/*.md' --en-gb --report",
    "start": "npm run develop",
    "serve": "gatsby serve",
    "clean": "gatsby clean",
    "test": "echo \"Write tests! -> https://gatsby.dev/unit-testing\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/aamct2/cooking-blog"
  },
  "bugs": {
    "url": "https://github.com/aamct2/cooking-blog/issues"
  }
}
